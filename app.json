    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "extra": {
      "apiKey": "AIzaSyCJ6-_vtEM2K0-rRhrny_XYK0wzgnBrGkk",
      "authDomain": "snow-patrol-e632f.firebaseapp.com",
      "projectId": "snow-patrol-e632f",
      "storageBucket": "snow-patrol-e632f.firebasestorage.app",
      "messagingSenderId": "1098354998146",
      "appId": "1:1098354998146:web:5471072ac22a92fdc86d9b"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],

      [
        "expo-font",
        {
